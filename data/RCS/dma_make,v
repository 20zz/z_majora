@#!/usr/local/bin/bash
#set -x
#
# $Id: dma_make,v,v 1.1.1.1 2003/12/06 23:45:38 wheeler Exp $
#

cp2() {
    if [ ! -e $1 ]; then
	echo "ファイルがありません $1"
    else
	if [ ! -e $2 -o $1 -nt $2 ]; then
	    echo    cp $1 $2
	    command cp $1 $2
	fi
    fi
}
cp() {
    SRC=$1
    DEST=$2
    SRCBASE=`basename $SRC`
    if [ -d $DEST ]; then
	cp2 $SRC $DEST/$SRCBASE
    else
	cp2 $SRC $DEST
    fi
}

#####	ＳＨＯＷ
     cp shape/TEXTURE2/E3/e3_select/e3_select.o space_world.o

#####	任天堂ロゴ
     cp shape/TEXTURE/DMA_MAKE/nintendo_rogo/nintendo_rogo.o .
#####	ファイル選択面
#     cp shape/TEXTURE/TITLE/title.o .
