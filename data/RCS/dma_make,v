1.53
log
@*** empty log message ***
@
text
@d4 1
a4 1
# $Id: dma_make,v,v 1.1.1.1 2003/12/06 23:45:38 wheeler Exp $
d8 8
a15 4
  if [ ! -e $2 -o $1 -nt $2 ]; then
    echo    cp $1 $2
    command cp $1 $2
  fi
d18 8
a25 8
  SRC=$1
  DEST=$2
  SRCBASE=`basename $SRC`
  if [ -d $DEST ]; then
    cp2 $SRC $DEST/$SRCBASE
  else
    cp2 $SRC $DEST
  fi
@


