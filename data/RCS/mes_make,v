text
@#!/usr/local/bin/bash
#set -x
#
# $Id: mes_make,v,v 1.1 2003/12/06 23:45:38 wheeler Exp $
#

cp2() {
    if [ ! -e $1 ]; then
	echo -e "\007\033[37;41m¥Õ¥¡¥¤¥ë¤¬¤¢¤ê¤Þ¤»¤ó $1\033[m"
    else
	if [ ! -e $2 -o $1 -nt $2 ]; then
	    echo    cp $1 $2
	    command cp $1 $2
	fi
    fi
}
cp() {
    SRC=$1
    DEST=$2
    SRCBASE=`basename $SRC`
    if [ -d $DEST ]; then
	cp2 $SRC $DEST/$SRCBASE
    else
	cp2 $SRC $DEST
    fi
}

